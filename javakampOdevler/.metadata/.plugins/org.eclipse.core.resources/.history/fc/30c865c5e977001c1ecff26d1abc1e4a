package hafta3Odev2;

public class UserManager {

	public void add(User user) {
		System.out.println(user.email +"hesabı sisteme kaydedildi");
	}
	
	public void addMultiple(User[] users) {
		
		for(User user:users) {
			add(user);//Gönd.arraydeki müş dolaşıp her bir müş için add i çalıştırır
		}
	}
}
