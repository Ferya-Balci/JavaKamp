package hafta3Odev2;

public class Main {

	public static void main(String[] args) {
		
		Instructor egitmen1=new Instructor();
		egitmen1.email="egitmen1@gmail.com";
		egitmen1.firstName="Engin ";
		egitmen1.lastName="Demiroğ";
		
		
		Student student1=new Student();
		student1.email="student1@gmail.com";
		student1.firstName="Ferya";
		student1.lastName="Balcı";
		
		UserManager userManager=new UserManager();
		
		User[] users= {egitmen1,student1};
		userManager.addMultiple(users);
		userManager.loginMultiple(users);
	}

}
